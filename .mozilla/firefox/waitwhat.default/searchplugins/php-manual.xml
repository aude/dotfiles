<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
    <ShortName>php</ShortName>
    <Description>PHP Manual</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="16" height="16">data:image/x-icon;base64,</Image>
    <SearchForm>https://secure.php.net/</SearchForm>
    <Url type="text/html" method="GET" template="https://secure.php.net/manual-lookup.php" resultDomain="php.net">
        <Param name="pattern" value="{searchTerms}"/>
    </Url>
    <!--<Url type="application/opensearchdescription+xml" method="GET" template="http://mycroftproject.com/updateos.php/id0/php_icon.xml" rel="self" resultDomain="mycroftproject.com"/>-->
</SearchPlugin>
