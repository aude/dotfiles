<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
    <ShortName>imdb</ShortName>
    <Description>IMDb - The Internet Movie Database</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="16" height="16">data:image/x-icon;base64,</Image>
    <SearchForm>http://www.imdb.com/</SearchForm>
    <Url type="text/html" method="GET" template="http://www.imdb.com/find" resultDomain="imdb.com">
        <Param name="s" value="all"/>
        <Param name="q" value="{searchTerms}"/>
    </Url>
    <!--<Url type="application/opensearchdescription+xml" method="GET" template="http://mycroftproject.com/updateos.php/id0/IMDb.xml" rel="self" resultDomain="mycroftproject.com"/>-->
</SearchPlugin>
