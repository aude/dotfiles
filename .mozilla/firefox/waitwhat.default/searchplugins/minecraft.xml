<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
    <ShortName>mc</ShortName>
    <Description>Minecraft Wiki</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="16" height="16">data:image/x-icon;base64,</Image>
    <Image width="24" height="24">data:image/x-icon;base64,</Image>
    <Image width="32" height="32">data:image/x-icon;base64,</Image>
    <SearchForm>http://minecraft.gamepedia.com/</SearchForm>
    <Url type="application/x-suggestions+json" method="GET" template="http://minecraft.gamepedia.com/api.php">
        <Param name="action" value="opensearch"/>
        <Param name="search" value="{searchTerms}"/>
    </Url>
    <Url type="text/html" method="GET" template="http://minecraft.gamepedia.com/index.php">
        <Param name="search" value="{searchTerms}"/>
        <Param name="title" value="Special:Search"/>
    </Url>
</SearchPlugin>
