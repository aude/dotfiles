<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
    <ShortName>wb</ShortName>
    <Description>Internet Archive: Wayback Machine</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="32" height="32">data:image/x-icon;base64,</Image>
    <SearchForm>https://archive.org/web/</SearchForm>
    <Url type="text/html" method="GET" template="https://web.archive.org/web/form-submit.jsp" resultDomain="web.archive.org">
        <Param name="type" value="urlquery"/>
        <Param name="url" value="{searchTerms}"/>
    </Url>
    <!--<Url type="application/opensearchdescription+xml" method="GET" template="http://mycroftproject.com/updateos.php/id0/archiveorg-webssl.xml" rel="self" resultDomain="mycroftproject.com"/>-->
</SearchPlugin>
