<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>DuckDuckGo HTML (POST)</os:ShortName>
<os:Description>DuckDuckGo (SSL enabled, no JavaScript required for results, uses POST instead of GET)</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>https://duckduckgo.com/html?kg=p&amp;kh=1</SearchForm>
<os:Url type="text/html" method="POST" template="https://duckduckgo.com/html" resultDomain="duckduckgo.com">
  <os:Param name="kg" value="p"/>
  <os:Param name="kh" value="1"/>
  <os:Param name="q" value="{searchTerms}"/>
<!--</os:Url><os:Url type="application/opensearchdescription+xml" method="GET" template="http://mycroftproject.com/updateos.php/id0/ddg_ssl_nojs_post.xml" rel="self" resultDomain="mycroftproject.com">-->
</os:Url>
<Alias>ddgh</Alias>
</SearchPlugin>
