<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
    <ShortName>yt</ShortName>
    <Description>YouTube</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="16" height="16">data:image/x-icon;base64,</Image>
    <Image width="32" height="32">data:image/x-icon;base64,</Image>
    <Image width="48" height="48">data:image/x-icon;base64,</Image>
    <Image width="96" height="96">data:image/x-icon;base64,</Image>
    <Image width="144" height="144">data:image/x-icon;base64,</Image>
    <SearchForm>https://www.youtube.com/</SearchForm>
    <Url type="text/html" method="GET" template="https://www.youtube.com/results" resultDomain="youtube.com">
        <Param name="search_query" value="{searchTerms}"/>
        <Param name="search" value="Search"/>
    </Url>
    <Url type="application/x-suggestions+json" method="GET" template="https://suggestqueries.google.com/complete/search" resultDomain="suggestqueries.google.com">
        <Param name="output" value="firefox"/>
        <Param name="ds" value="yt"/>
        <Param name="q" value="{searchTerms}"/>
    </Url>
    <!--<Url type="application/opensearchdescription+xml" method="GET" template="http://mycroftproject.com/updateos.php/id0/youtube.xml" rel="self" resultDomain="mycroftproject.com"/>-->
</SearchPlugin>
